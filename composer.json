{
    "prefer-stable": true,
    "name": "fpoirotte/pssht",
    "description": "SSH server library written in PHP",
    "keywords": ["SSH", "server", "secure", "shell"],
    "homepage": "https://github.com/fpoirotte/pssht",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Poirotte",
            "email": "clicky@erebot.net",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/fpoirotte/pssht/issues",
        "irc": "irc://irc.iiens.net/Erebot",
        "source": "https://github.com/fpoirotte/pssht"
    },
    "require": {
        "php": ">=5.3.3",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-gmp": "*",
        "ext-openssl": "*",
        "ext-reflection": "*",
        "ext-sockets": "*",
        "ext-spl": "*",
        "symfony/dependency-injection": "*",
        "symfony/config": "*",
        "erebot/plop": ">=0.6"
    },
    "require-dev": {
        "ext-hash": "*",
        "ext-http": "*",
        "ext-mcrypt": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "phing/phing": "*",
        "phpmd/phpmd": "*",
        "sebastian/phpcpd": "*",
        "phpunit/phpunit": "<4",
        "squizlabs/php_codesniffer": "*",
        "erebot/buildenv": "~1.2.0",
        "fpoirotte/natives4doxygen": "*"
    },
    "suggest": {
        "ext-hash": "Add support for various additional algorithms",
        "ext-http": "Add zlib-compression support (>= 1.0)",
        "ext-mcrypt": "Add support for most encryption algorithms",
        "ext-pcntl": "Handle signals gracefully",
        "ext-posix": "Improve detection of user running pssht"
    },
    "autoload": {
        "psr-4": {
            "fpoirotte\\Pssht\\": "src/"
        }
    },
    "bin": ["bin/pssht"],
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
